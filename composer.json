{
    "name": "aplus/database-extra",
    "description": "Aplus Framework Database Extra Library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "database",
        "database-extra",
        "extra",
        "migration",
        "migrator",
        "seed",
        "seeder"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/database-extra",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/database-extra/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://gitlab.com/aplus-framework/libraries/database-extra",
        "docs": "https://docs.aplus-framework.com/guides/libraries/database-extra/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.0",
        "aplus/autoload": "^1.7",
        "aplus/database": "^2.8"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.10",
        "ergebnis/composer-normalize": "^2.15",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Framework\\Database\\Extra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Database\\Extra\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
